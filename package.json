{
  "name": "source-code",
  "author": "Bachitter Chahal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bachitterch/source-code.git"
  },
  "bugs": {
    "url": "https://github.com/bachitterch/source-code/issues"
  },
  "homepage": "https://github.com/bachitterch/source-code#readme",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,css,scss}'"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
